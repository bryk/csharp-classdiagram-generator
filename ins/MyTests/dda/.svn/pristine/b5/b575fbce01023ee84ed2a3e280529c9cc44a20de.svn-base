<Window x:Class="Project.GUI.ChooseTypesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SearchingWizard" Height="350" Width="525" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid Background="Gray">
        <Grid Height="285" HorizontalAlignment="Left" Margin="8,12,0,0" Name="grid2" VerticalAlignment="Top" Width="90">
            <Image Height="273" HorizontalAlignment="Left" Margin="6,6,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="78" Source="/Project.GUI;component/Image.jpg" />
        </Grid>
        <Grid Height="72" HorizontalAlignment="Left" Margin="109,12,0,0" Name="grid1" VerticalAlignment="Top" Width="382">
            <Label Content="Please select the depth of searching" Height="60" HorizontalAlignment="Left" Margin="6,6,0,0" Name="label2" VerticalAlignment="Top" Width="370" Foreground="White"/>
        </Grid>
        <Grid Height="41" HorizontalAlignment="Left" Margin="103,262,0,0" Name="grid3" VerticalAlignment="Top" Width="383">
            <Button Click="button1_Click" Content="Back" Height="23" HorizontalAlignment="Left" Margin="18,12,0,0" Name="button1" VerticalAlignment="Top" Width="75"/>
            <Button Click="button2_Click" Content="Next" Height="23" HorizontalAlignment="Left" Margin="113,12,0,0" Name="button2" VerticalAlignment="Top" Width="75" />
            <Button Content="Process" Height="23" HorizontalAlignment="Right" Margin="0,12,101,0" Name="button3" VerticalAlignment="Top" Width="75" IsEnabled="False" />
            <Button Click="button4_Click" Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="302,12,0,0" Name="button4" VerticalAlignment="Top" Width="75" />
        </Grid>
        <Grid Height="146" HorizontalAlignment="Left" Margin="109,92,0,0" Name="grid4" VerticalAlignment="Top" Width="379">
            <Label Height="140" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Width="373"></Label>
            <Grid Height="100" HorizontalAlignment="Left" Margin="6,29,0,0" Name="grid5" VerticalAlignment="Top" Width="81">
                <RadioButton Foreground="White" Content="Section" HorizontalAlignment="Left" Margin="8,6,0,78" Name="radioButton1" Checked="radioButton1_Checked" />
                <RadioButton Foreground="White" Content="Thread" Height="16" HorizontalAlignment="Left" Margin="8,28,0,0" Name="radioButton2" VerticalAlignment="Top" Checked="radioButton2_Checked" />
                <RadioButton Foreground="White" Content="Post" Height="16" HorizontalAlignment="Left" Margin="8,50,0,0" Name="radioButton3" VerticalAlignment="Top" Checked="radioButton3_Checked" />
            </Grid>
            <Image Height="134" HorizontalAlignment="Left" Margin="144,6,0,0" Name="image2" Stretch="Fill" VerticalAlignment="Top" Width="227" />
        </Grid>
    </Grid>
</Window>
