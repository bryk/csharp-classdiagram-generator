<Window x:Class="Project.GUI.SelectAttributesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SearchingWizard" Height="350" Width="525" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid Background="Gray">
        <Grid Height="285" HorizontalAlignment="Left" Margin="8,12,0,0" Name="grid2" VerticalAlignment="Top" Width="90">
            <Image Height="273" HorizontalAlignment="Left" Margin="6,6,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="78" Source="/Project.GUI;component/Image.jpg" />
        </Grid>
        <Grid Height="72" HorizontalAlignment="Left" Margin="109,12,0,0" Name="grid1" VerticalAlignment="Top" Width="382">
            <Label Foreground="White" Content="Please select attributes of the Post" Height="60" HorizontalAlignment="Left" Margin="6,6,0,0" Name="label2" VerticalAlignment="Top" Width="370" />
        </Grid>
        <Grid Height="41" HorizontalAlignment="Left" Margin="103,262,0,0" Name="grid3" VerticalAlignment="Top" Width="383">
            <Button Click="button1_Click" Content="Back" Height="23" HorizontalAlignment="Left" Margin="18,12,0,0" Name="button1" VerticalAlignment="Top" Width="75"/>
            <Button Click="button2_Click" Content="Next" Height="23" HorizontalAlignment="Left" Margin="113,12,0,0" Name="button2" VerticalAlignment="Top" Width="75" IsEnabled="False" />
            <Button Click="button3_Click" Content="Process" Height="23" HorizontalAlignment="Right" Margin="0,12,101,0" Name="button3" VerticalAlignment="Top" Width="75" />
            <Button Click="button4_Click" Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="302,12,0,0" Name="button4" VerticalAlignment="Top" Width="75" />
        </Grid>
        <Grid Height="176" HorizontalAlignment="Left" Margin="109,92,0,0" Name="grid4" VerticalAlignment="Top" Width="379">
            <ListBox Height="110" HorizontalAlignment="Left" Margin="12,6,0,0" Name="listBox1" VerticalAlignment="Top" Width="146" />
            <Label Background="White" Content="Query" Height="48" HorizontalAlignment="Left" Margin="12,122,0,0" Name="label1" VerticalAlignment="Top" Width="344" IsEnabled="False" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="201,54,0,0" Name="textBox1" VerticalAlignment="Top" Width="155" Text="" />
            <Button Content="=" Height="23" HorizontalAlignment="Left" Name="button5" VerticalAlignment="Top" Width="30" Margin="164,54,0,0" Click="button5_Click" />
            <Label HorizontalContentAlignment="Center" Content="Please, write value" Height="28" HorizontalAlignment="Center" Margin="200,20,22,0" Name="label3" VerticalAlignment="Top" Width="155" Foreground="White" />
        </Grid>
    </Grid>
</Window>
